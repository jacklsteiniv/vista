<div id = "user-page">
  <% provide(:title, @user.name) %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
       <h1><%= @user.name %></h1>
        <br>
       <h1>
        <%= @user.id %>
        <%= @user.email %>
        <br>
        <img class = "profpic" src = "<%= @user.image_url%>">
      </h1>
      </section>
    </aside>
  </div>
</div>

